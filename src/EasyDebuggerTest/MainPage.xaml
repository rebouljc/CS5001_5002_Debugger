                <NavigationViewItem Content="Start" x:Name="Start" Tag="StartPage"/>
                <NavigationViewItem Content="Processes Viewer" x:Name="B" />
                <NavigationViewItem Content="Source Code Viewer" x:Name="C" />
            </NavigationView.MenuItems>
        </NavigationView>

       
    </Grid>

</Page>
